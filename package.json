    "start:dev": "npm run start -- --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.8",
    "@nestjs/core": "^11.0.8",
    "@nestjs/platform-express": "^11.0.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "printWidth": 140,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
